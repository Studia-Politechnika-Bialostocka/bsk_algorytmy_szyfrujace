<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="main_window">
    <property name="geometry">640x395</property>
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Label" id="RSA_main_label">
        <property name="font">{Bahnschrift SemiLight SemiConde} 20 {bold}</property>
        <property name="text" translatable="yes">Public Key Cryptography using RSA algorithm</property>
        <layout manager="pack">
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="label4">
        <property name="font">{Bahnschrift SemiLight SemiConde} 11 {}</property>
        <property name="text" translatable="yes">Enter two prime numbers below (P, Q), then press calculate:</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">20</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="public_private_key_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">20</property>
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="label10">
            <property name="font">{Bahnschrift SemiLight SemiConde} 16 {}</property>
            <property name="text" translatable="yes">P</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="public_key_input">
            <property name="justify">center</property>
            <property name="textvariable">int:p_var</property>
            <property name="width">12</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="label14">
            <property name="font">{Bahnschrift SemiLight SemiConde} 16 {}</property>
            <property name="text" translatable="yes">Q</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entry5">
            <property name="justify">center</property>
            <property name="textvariable">int:q_var</property>
            <property name="width">12</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="button4">
            <property name="command" type="command" cbtype="simple">generate_public_private_key_call</property>
            <property name="text" translatable="yes">Calculate</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="ipadx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="frame4">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">20</property>
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="label19">
            <property name="font">{Bahnschrift SemiLight SemiConde} 16 {}</property>
            <property name="text" translatable="yes">Private key (E, N)</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="generated_private_key">
            <property name="justify">center</property>
            <property name="textvariable">string:private_key_var</property>
            <property name="width">15</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="label18">
            <property name="font">{Bahnschrift SemiLight SemiConde} 16 {}</property>
            <property name="text" translatable="yes">Public key (D, N)</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="generated_public_key">
            <property name="justify">center</property>
            <property name="textvariable">string:public_key_var</property>
            <property name="width">15</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="input_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">20</property>
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="label1">
            <property name="font">{Bahnschrift SemiLight SemiConde} 16 {}</property>
            <property name="text" translatable="yes">Message</property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="code_input">
            <property name="textvariable">string:message_var</property>
            <property name="width">60</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="fill">y</property>
              <property name="padx">10</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="encrypt_button">
            <property name="command" type="command" cbtype="simple">encrypt_message_call</property>
            <property name="text" translatable="yes">Encrypt</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="ipadx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="decrypt_button">
            <property name="command" type="command" cbtype="simple">decrypt_message_call</property>
            <property name="text" translatable="yes">Decrypt</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="ipadx">5</property>
              <property name="padx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="frame2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">20</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="label6">
            <property name="font">{Bahnschrift SemiLight SemiConde} 11 {}</property>
            <property name="text" translatable="yes">Encrypt/Decrypt file:</property>
            <layout manager="pack">
              <property name="anchor">w</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="button9">
            <property name="command" type="command" cbtype="simple">select_file_call</property>
            <property name="text" translatable="yes">Select file</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="ipadx">20</property>
              <property name="padx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="button7">
            <property name="command" type="command" cbtype="simple">encrypt_file_call</property>
            <property name="text" translatable="yes">Encrypt</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="ipadx">5</property>
              <property name="padx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="button8">
            <property name="command" type="command" cbtype="simple">decrypt_file_call</property>
            <property name="text" translatable="yes">Decrypt</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="ipadx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="output_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="padx">20</property>
          <property name="pady">50</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="label27">
            <property name="font">{Bahnschrift SemiLight SemiConde} 16 {}</property>
            <property name="text" translatable="yes">Output   </property>
            <layout manager="pack">
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="code_output">
            <property name="textvariable">string:output_var</property>
            <property name="width">80</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="fill">y</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
